{
  "name": "@dhzh/eslint-config",
  "type": "module",
  "version": "0.25.0",
  "packageManager": "pnpm@10.6.1",
  "description": "Lyle's ESLint config",
  "author": "Lyle Zheng <dhzhme@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/tinywaves/eslint-config",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "nr typegen && tsup --clean --dts",
    "stub": "tsup",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build",
    "watch": "tsup --watch",
    "lint": "eslint",
    "lint-fix": "eslint --fix .",
    "typegen": "tsx scripts/typegen.ts",
    "prepack": "nr build",
    "release": "bumpp && pnpm publish --access=public",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@prettier/plugin-xml": "^3.4.1",
    "@unocss/eslint-plugin": ">=0.50.0",
    "astro-eslint-parser": "^1.0.2",
    "eslint": "^9.10.0",
    "eslint-plugin-astro": "^1.2.0",
    "eslint-plugin-format": ">=0.1.0",
    "eslint-plugin-solid": "^0.14.3",
    "eslint-plugin-svelte": ">=2.35.1",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-slidev": "^1.0.5",
    "svelte-eslint-parser": ">=0.37.0"
  },
  "peerDependenciesMeta": {
    "@prettier/plugin-xml": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "prettier-plugin-astro": {
      "optional": true
    },
    "prettier-plugin-slidev": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.0.0",
    "@clack/prompts": "^0.10.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint-react/eslint-plugin": "^1.31.0",
    "@eslint/markdown": "^6.3.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitest/eslint-plugin": "^1.1.36",
    "ansis": "^3.17.0",
    "cac": "^6.7.14",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-antfu": "^3.1.0",
    "eslint-plugin-command": "^3.1.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-jsonc": "^2.19.1",
    "eslint-plugin-n": "^17.16.2",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^4.9.0",
    "eslint-plugin-pnpm-catalogs": "^0.1.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.0.0",
    "eslint-plugin-yml": "^1.17.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "parse-gitignore": "^2.0.0",
    "toml-eslint-parser": "^0.10.0",
    "vue-eslint-parser": "^10.1.1",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@antfu/ni": "^23.3.1",
    "@eslint/config-inspector": "^1.0.2",
    "@prettier/plugin-xml": "^3.4.1",
    "@stylistic/eslint-plugin-migrate": "^4.2.0",
    "@types/node": "^22.13.9",
    "@unocss/eslint-plugin": "^66.0.0",
    "astro-eslint-parser": "^1.2.1",
    "bumpp": "^10.0.3",
    "bundle-require": "^5.1.0",
    "eslint": "^9.21.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^3.0.3",
    "eslint-typegen": "^2.0.0",
    "jiti": "^2.4.2",
    "lint-staged": "^15.4.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-slidev": "^1.0.5",
    "simple-git-hooks": "^2.11.1",
    "svelte-eslint-parser": "^1.0.1",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "resolutions": {
    "@eslint-community/eslint-utils": "^4.4.1",
    "@typescript-eslint/utils": "^8.26.0",
    "eslint": "^9.21.0",
    "tsx": "^4.19.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}

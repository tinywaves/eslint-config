{
  "name": "@dhzh/eslint-config",
  "type": "module",
  "version": "1.0.4",
  "packageManager": "pnpm@10.9.0",
  "description": "Lyle's ESLint config",
  "author": {
    "name": "Lyle Zheng",
    "email": "dhzhme@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/tinywaves/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinywaves/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/tinywaves/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "scripts": {
    "build": "pnpm run typegen && tsup --clean --dts",
    "build:inspector": "pnpm run build && npx @eslint/config-inspector build",
    "dev": "pnpm run typegen && npx @eslint/config-inspector --config eslint.config.ts",
    "lint": "eslint",
    "lint-fix": "eslint --fix .",
    "prepack": "pnpm run build",
    "prepare": "simple-git-hooks",
    "release": "bumpp && pnpm publish --access=public",
    "typecheck": "tsc --noEmit",
    "typegen": "tsx scripts/eslint-typegen.ts"
  },
  "peerDependencies": {
    "@prettier/plugin-xml": "^3.4.1",
    "eslint": "^9.10.0"
  },
  "peerDependenciesMeta": {
    "@prettier/plugin-xml": {
      "optional": true
    }
  },
  "dependencies": {
    "@clack/prompts": "^0.10.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint-react/eslint-plugin": "^1.48.5",
    "@eslint/js": "^9.25.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@unocss/eslint-config": "66.1.0-beta.12",
    "ansis": "^3.17.0",
    "cac": "^6.7.14",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-hyoban": "^0.6.1",
    "eslint-plugin-import-x": "^4.11.0",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-package-json": "^0.30.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.1",
    "eslint-plugin-react-google-translate": "^0.1.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-yml": "^1.18.0",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "toml-eslint-parser": "^0.10.0",
    "typescript-eslint": "^8.31.0",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.0.2",
    "@prettier/plugin-xml": "^3.4.1",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/node": "^22.15.2",
    "bumpp": "^10.1.0",
    "bundle-require": "^5.1.0",
    "eslint": "^9.25.1",
    "eslint-typegen": "^2.1.0",
    "lint-staged": "^15.5.1",
    "simple-git-hooks": "^2.13.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}

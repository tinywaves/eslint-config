{
  "name": "@dhzh/eslint-config",
  "type": "module",
  "version": "1.23.2",
  "packageManager": "pnpm@10.28.2",
  "description": "Lyle's ESLint config",
  "author": {
    "name": "Lyle Zheng",
    "email": "dhzhme@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/tinywaves/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinywaves/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/tinywaves/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "scripts": {
    "build": "pnpm run typegen && tsup --clean --dts",
    "build:inspector": "pnpm run build && npx @eslint/config-inspector build",
    "dev": "pnpm run typegen && npx @eslint/config-inspector --config eslint.config.ts",
    "lint": "eslint",
    "lint-fix": "eslint --fix .",
    "prepack": "pnpm run build",
    "prepare": "simple-git-hooks",
    "release": "bumpp",
    "typecheck": "tsc --noEmit",
    "typegen": "tsx scripts/eslint-typegen.ts"
  },
  "peerDependencies": {
    "@prettier/plugin-xml": "^3.4.1",
    "eslint": "^9.39.2"
  },
  "peerDependenciesMeta": {
    "@prettier/plugin-xml": {
      "optional": true
    }
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
    "@eslint-react/eslint-plugin": "^2.8.1",
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@unocss/eslint-config": "66.6.0",
    "ansis": "^4.2.0",
    "cac": "^6.7.14",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-antfu": "^3.1.3",
    "eslint-plugin-format": "^1.3.1",
    "eslint-plugin-hyoban": "^0.6.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-package-json": "^0.88.2",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-google-translate": "^0.1.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-regexp": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "4.0.0-beta.0",
    "eslint-plugin-toml": "^1.0.3",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vue": "^10.7.0",
    "eslint-plugin-yml": "^3.0.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "globals": "^17.2.0",
    "jsonc-eslint-parser": "^2.4.2",
    "local-pkg": "^1.1.2",
    "toml-eslint-parser": "^1.0.3",
    "typescript-eslint": "^8.54.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.4.2",
    "@prettier/plugin-xml": "^3.4.2",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/node": "^22.19.7",
    "bumpp": "^10.4.0",
    "bundle-require": "^5.1.0",
    "eslint": "^9.39.2",
    "eslint-typegen": "^2.3.0",
    "lint-staged": "^16.2.7",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^4.1.18",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
